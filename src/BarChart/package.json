{
  "name": "one-vscode-extension",
  "displayName": "Backend .json visualization",
  "description": "Visualize the backend output(.json)",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "homepage": "https://github.com/Samsung/ONE-vscode",
  "categories": [
    "Extension Packs",
    "Visualization"
  ],
  "keywords": [
    "ONE vscode",
    "samsung",
    "TheONE",
    "SSAFY"
  ],
  "publisher": "TheONE",
  "activationEvents": [
    "onCommand:one-vscode-barchart.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "one-vscode-barchart.start",
        "title": "one vscode barchart",
        "category": "ONE"
      }
    ]
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "styled-components": "^5.3.1",
    "terser": "3.16.0",
    "web-vitals": "^2.1.0"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "vscode:prepublish": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts start",
    "build": "node scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch-build": "nodemon --watch src --ext js,tsx,ts --exec \"yarn run build\""
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "nodemon": "^2.0.7",
    "react-scripts": "^2.1.3",
    "rewire": "^4.0.1",
    "typescript": "^4.2.2",
    "vscode": "^1.1.37"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Samsung/ONE-vscode"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
